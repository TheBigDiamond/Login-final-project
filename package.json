{
  "name": "accenture_login",
  "private": true,
  "version": "1.0.0",
  "description": "Full-stack application with Vue.js frontend and Node.js/Express backend",
  "main": "backend/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "client": "vite --host",
    "server": "node backend/index.js",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "dev:all": "concurrently -k -s first -n 'SERVER,CLIENT' -c 'bgBlue.bold,bgGreen.bold' \"npm run server\" \"npm run client\"",
    "start": "concurrently -k -s first -n 'SERVER,CLIENT' -c 'bgBlue.bold,bgGreen.bold' \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.1.7",
    "concurrently": "^8.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "backend/prisma/schema.prisma"
  }
}
